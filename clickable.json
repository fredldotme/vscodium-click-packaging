{
  "clickable_minimum_required": "8.4.0",
  "framework": "ubuntu-touch-24.04-1.x",
  "builder": "custom",
  "kill": "codium",
  "build": "bash ${ROOT}/build.sh ${ARCH} ${SDK_FRAMEWORK}",
  "dependencies_host": [
    "gtk-doc-tools",
    "libatk1.0-dev",
    "libatk-bridge2.0-dev",
    "libcairo2-dev",
    "libepoxy-dev",
    "libgirepository1.0-dev",
    "libmount-dev",
    "libpango1.0-dev",
    "libwayland-dev",
    "libxi-dev",
    "python3-pip",
    "wget",
    "libtool"
  ],
  "dependencies_target": [
    "libfreetype6",
    "libpng16-16",
    "libgtk3-nocsd0",
    "gtk3-nocsd"
  ],
  "install_bin": [
    "/usr/bin/gtk3-nocsd"
  ],
  "install_lib": [
    "/usr/lib/${ARCH_TRIPLET}/libgtk3-nocsd*.so*"
  ]
}
